<jxb:bindings version="1.0" 
  xmlns:jxb="http://java.sun.com/xml/ns/jaxb" 
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" 
  jxb:extensionBindingPrefixes="xjc">
    <jxb:bindings schemaLocation="struts-config.xsd" node="/xs:schema">
        <jxb:bindings node="//xs:element[@name='action']//xs:complexType//xs:attribute[@name='forward']">
            <jxb:property name="action-forward"/>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>